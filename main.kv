<MainScreen>
	name:'MainScreen'
	id: AB_name 
	id: computer_name
	id: player_name
	id: play_way,analy
	canvas:
	    Color:
	        rgba:0.5,0.5,0.5,1
	    Rectangle:
        	pos: self.pos
        	size: self.size
	Button:
		pos_hint:{'x':.2,'y':.7}
		size_hint:0.6,0.3
		background_normal: "title.png"
		background_down: 'title.png'
		id: AB_name 

	Button:
		on_release:app.root.current="tocomputer"
		pos_hint:{'x':.25,'y':.6}
		size_hint:0.5,0.12
		background_normal: 'button_back.png'
		background_down: 'button_back.png'
		id: computer_name
	Button:
		on_release:app.root.current="toplayer"
		on_release: root.count_down()
		pos_hint:{'x':.25,'y':.46}
		size_hint:0.5,0.12
		background_normal: 'button_back.png'
		background_down: 'button_back.png'
